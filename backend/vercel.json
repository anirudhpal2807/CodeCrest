{
  "version": 2,
  "builds": [
    {
      "src": "vite.config.js",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_MONGODB_URI": "@mongodb_uri",
    "VITE_JWT_KEY": "@jwt_key",
    "VITE_JUDGE0_KEY": "@judge0_key",
    "VITE_REDIS_URL": "@redis_url"
  }
}
