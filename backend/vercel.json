{
  "version": 2,
  "builds": [
    {
      "src": "src/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/index.js"
    }
  ],
  "env": {
    "MONGODB_URI": "@mongodb_uri",
    "JWT_KEY": "@jwt_key",
    "JUDGE0_KEY": "@judge0_key",
    "REDIS_URL": "@redis_url",
    "CORS_ORIGIN": "@cors_origin"
  },
  "functions": {
    "src/index.js": {
      "maxDuration": 30
    }
  }
}
